{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "create-block/wpurlgallery",
	"version": "0.1.0",
	"title": "URL Gallery",
	"category": "widgets",
	"icon": "format-gallery",
	"description": "Block to create a Gallery from URLs",
	"supports": {
		"html": false
	},
	"textdomain": "wpurlgallery",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"images": {
			"type": "array",
			"source": "text",
			"default": ["https://http.cat/404", "https://http.cat/200", "https://http.cat/500", "https://http.cat/100"]
		}

	}
}
